#!/usr/bin/env python
# -*- coding: utf-8 -*-

import numpy as np
from src.lib.common import get_data_dir
from src.tcofils.tcofils import generate_dataset

# user_id movie_id rating timestamp
ratings = np.genfromtxt(get_data_dir()+"/ml-100k/ml-100k/u.data", delimiter='\t')

supervised_learning_dataset = generate_dataset(
	ratings,
	normalization_method="user_avg",
	method="svg",
	num_factors=2, 
	include_time="naive")

print(supervised_learning_dataset.shape)
